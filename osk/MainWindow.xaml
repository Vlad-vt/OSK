                <Grid.RowDefinitions>
                    <RowDefinition Height="7"/>
                    <RowDefinition Height="80"/>
                    <RowDefinition Height="7"/>
                    <RowDefinition Height="80"/>
                    <RowDefinition Height="7"/>
                    <RowDefinition Height="80"/>
                    <RowDefinition Height="7"/>
                    <RowDefinition Height="80"/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="9"/>
                    <ColumnDefinition Width="27"/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <StackPanel Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="3" Orientation="Horizontal">
