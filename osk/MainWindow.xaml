                        </TextBlock>
                    </Button>
                    <Button Command="{Binding PressS}" Background="{Binding PressSBGColor}" BorderBrush="{Binding PressSBBColor}" Style="{StaticResource RoundCorner}" Width="80" Height="75" Margin="18,0,0,0">
