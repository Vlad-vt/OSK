                        </TextBlock>
                    </Button>
                    <Button Command="{Binding PressX}" Background="{Binding PressXBGColor}" BorderBrush="{Binding PressXBBColor}" Style="{StaticResource RoundCorner}" Width="80" Height="75" Margin="18,0,0,0">
