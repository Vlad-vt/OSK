            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="7"/>
                    <RowDefinition Height="80"/>
                    <RowDefinition Height="7"/>
                    <RowDefinition Height="80"/>
                    <RowDefinition Height="7"/>
                    <RowDefinition Height="80"/>
                    <RowDefinition Height="7"/>
                    <RowDefinition Height="80"/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="9"/>
                    <ColumnDefinition Width="27"/>
                    <ColumnDefinition/>
                    <ColumnDefinition Width="177"/>
                    <ColumnDefinition Width="10"/>
                </Grid.ColumnDefinitions>
