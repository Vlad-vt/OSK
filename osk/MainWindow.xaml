                    </Button>
                    <Button Command="{Binding PressZ}" Background="{Binding PressZBGColor}" BorderBrush="{Binding PressZBBColor}" Style="{StaticResource RoundCorner}" Width="80" Height="75" Margin="18,0,0,0">
